  <!-- ==========BloodTub!!!!==============-->
  <ThingDef ParentName="BedBase">
    <defName>VBloodTubV</defName>
    <label>Bloodtub</label>
    <description>It's nothing.:)</description>
    <graphicData>
      <texPath>BloodTub/BloodTub</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(1,2)</drawSize>
      <damageData>
        <rect>(0,0.05,1,1.95)</rect>
      </damageData>
    </graphicData>
    <comps>
      <li Class="CompProperties_AffectedByFacilities">
        <linkableFacilities>
          <li>EndTable</li>
          <li>Dresser</li>
        </linkableFacilities>
      </li>
    </comps>
    <statBases>
      <MaxHitPoints>140</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <Beauty>1</Beauty>
      <WorkToBuild>800</WorkToBuild>
      <Mass>50</Mass>
      <BedRestEffectiveness>1</BedRestEffectiveness>
      <Comfort>0.4</Comfort>
      <ImmunityGainSpeedFactor>1.07</ImmunityGainSpeedFactor>
      <SurgerySuccessChanceFactor>1</SurgerySuccessChanceFactor>
    </statBases>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <size>(1,2)</size>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>45</costStuffCount>
    <building>
      <bed_showSleeperBody>False</bed_showSleeperBody>
      <bed_healPerDay>4</bed_healPerDay>
    </building>
    <fillPercent>0.4</fillPercent>
    <pathCost>60</pathCost>
    <staticSunShadowHeight>0.17</staticSunShadowHeight>
    <castEdgeShadows>True</castEdgeShadows>
    <designationHotKey>Misc2</designationHotKey>
    <techLevel>Medieval</techLevel>
    <researchPrerequisites>
      <li>ComplexFurniture</li>
    </researchPrerequisites>
    <uiIconScale>0.90</uiIconScale>
  </ThingDef>
  <!-- ==========Hook bed!!!!==============-->
  <ThingDef ParentName="BedBase">
    <defName>VHookbedAV</defName>
    <label>Hook bed</label>
    <description>Not comfortable.</description>
    <thingClass>Building_Bed</thingClass>
    <graphicData>
      <texPath>Hook/hookbedA</texPath>
      <shaderType>CutoutComplex</shaderType>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,2)</drawSize>
    </graphicData>
    <altitudeLayer>ItemImportant</altitudeLayer>
    <statBases>
      <WorkToBuild>0</WorkToBuild>
      <Comfort>0.1</Comfort>
      <SurgerySuccessChanceFactor>0.7</SurgerySuccessChanceFactor>
      <Mass>5</Mass>
    </statBases>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <useHitPoints>False</useHitPoints>
    <size>(1,2)</size>
    <designationCategory>Misc</designationCategory>
    <designatorDropdown>VeeHookBed</designatorDropdown>
    <passability>Standable</passability>
    <drawGUIOverlay>True</drawGUIOverlay>
    <placeWorkers Inherit="False" />
    <defaultPlacingRot>North</defaultPlacingRot>
    <building>
      <bed_showSleeperBody>True</bed_showSleeperBody>
      <sowTag>SupportPlantsOnly</sowTag>
      <canPlaceOverImpassablePlant>false</canPlaceOverImpassablePlant>
      <ai_chillDestination>false</ai_chillDestination>
      <isEdifice>false</isEdifice>
    </building>
    <clearBuildingArea>false</clearBuildingArea>
    <tradeability>None</tradeability>
  </ThingDef>
